{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "standard"
  ],
  "parser": "babel-eslint",
  "plugins": [
      "babel",
      "jest"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "warn",
          4,
          {
              "SwitchCase": 1
          }
      ],
      "semi": "off",
      "space-before-function-paren": [
          "warn",
          {
              "anonymous": "ignore",
              "named": "ignore",
              "asyncArrow": "ignore"
          }
      ],
      "comma-dangle": [
          "warn",
          {
              "arrays": "ignore",
              "objects": "ignore",
              "imports": "ignore",
              "exports": "ignore",
              "functions": "ignore"
          }
      ]
  },
  "settings": {}
}